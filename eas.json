{
  "cli": {
    "version": ">= 5.0.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "useGenericFormats": true,
        "applicationIdForGradleVariable": true,
        "resourceClass": "default"
      },
      "ios": {
        "buildType": "simulator",
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": true,
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": true,
        "NODE_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "useGenericFormats": true,
        "applicationIdForGradleVariable": true,
        "resourceClass": "default"
      },
      "ios": {
        "simulator": false,
        "resourceClass": "default"
      },
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": true,
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": true,
        "NODE_ENV": "preview"
      },
      "autoSubmit": false
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "apk",
        "useGenericFormats": true,
        "applicationIdForGradleVariable": true,
        "gradleCommand": ":app:bundleRelease",
        "resourceClass": "large"
      },
      "ios": {
        "simulator": false,
        "resourceClass": "large"
      },
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": true,
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": true,
        "NODE_ENV": "production"
      },
      "autoSubmit": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "$APPLE_ID",
        "appleTeamId": "$APPLE_TEAM_ID",
        "ascAppId": "$ASC_APP_ID"
      },
      "android": {
        "serviceAccountJsonPath": "$ANDROID_SERVICE_ACCOUNT_JSON_PATH",
        "track": "production"
      }
    }
  },
  "resourceClass": {
    "default": "default",
    "large": "large"
  }
}
